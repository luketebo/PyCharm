   "source": [
    "import numpy as np\n",
    "X = np.mat([[4,2],[0,2],[-2,0],[-2,-4]])\n",
    "print(X)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[[24 16]\n",
      " [16 24]]\n"
     ]
    }
   ],
   "source": [
    "# print(X.T)\n",
    "print(X.T * X)\n"
   ],
   "metadata": {
    "collapsed": false
   }
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[40.  8.]\n"
     ]
    }
   ],
   "source": [
    "tr, W = np.linalg.eig(X.T * X)\n",
    "print(tr)"
   ],
   "metadata": {
    "collapsed": false
   }
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "outputs": [
